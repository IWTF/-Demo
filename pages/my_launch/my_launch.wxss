/* pages/my_launch/my_launch.wxss */
.header{
  background-image: url("https://www.cayani.cn/usr/uploads/2018/08/2714390371.jpg");          
  additive-symbols: no-repeat;
  background-size: 100% 100%;
  width: 750rpx;
  height: 360rpx;
}

.launch{
  font-size: 40rpx;
  color: #fff;
  padding: 150rpx 0rpx 0 40rpx;
}

.totle{
  font-size: 30rpx;
  color: #fff;
  padding: 20px 0rpx;
  margin-left: 40rpx;
}

.content{
  height: 100%;
  width: 100%;
}

.content image{
  height: 120rpx;
  width: 120rpx;
  margin: auto;
  margin-top: 70rpx;
  margin-left: 310rpx;
}

.content view{
  font-size: 28rpx;
  padding: 30rpx 0;
  margin-left: 310rpx;
}

.post_container{
  margin: 0rpx 40rpx;
  border-bottom: 4px solid #e2e2e2;
  background-color: #fff;
  padding: 10rpx 0 15rpx 0;
}

.sort{
  margin:10rpx 0rpx;
}

.sort text{
  font-size:26rpx;
  margin-left:8rpx;
  padding:5rpx;
  border-radius: 4px;
}

.post_img{
  height:350rpx;
  width:100%;
  border-radius: 15rpx;
  margin: auto;
}

.inf view{
  font-size:26rpx;
  color:#999;
}
